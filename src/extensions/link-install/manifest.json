{
  "key": "",
  "manifest_version": 2,
  "name": "Link Download Extension",
  "version": "0.0.1",
  "minimum_chrome_version": "88",
  "description": "An extension which can be downloaded by clicking on a link via policy ExtensionInstallSources",
  "icons": {
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "<all_urls>",
    "activeTab",
    "browsingData",
    "contentSettings",
    "cookies",
    "enterprise.hardwarePlatform",
    "history",
    "idle",
    "management",
    "nativeMessaging",
    "pageCapture",
    "privacy",
    "storage",
    "tabs",
    "webNavigation"
  ],
  "externally_connectable": {
    "matches": [
      "http://localhost:5000/*"
    ]
  },
  "update_url": "http://localhost:5000/extensions/linkclcacemnpnaidfknmbacmkcecjfd/update.xml"
}
